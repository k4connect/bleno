  "readmeFilename": "README.md",
  "devDependencies": {
    "jshint": "2.10.2",
    "should": "13.2.3",
    "mocha": "6.2.2",
    "node-blink1": "0.2.4"
  },
  "dependencies": {
    "@abandonware/bluetooth-hci-socket": "0.5.3-3",
    "debug": "4.1.1"
  },
  "optionalDependencies": {
    "bplist-parser": "0.1.1",
    "xpc-connection": "0.1.4"
  }
